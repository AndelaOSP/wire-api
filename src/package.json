{
  "name": "wire-api",
  "version": "1.0.0",
  "description": "Wire API",
  "main": "index.js",
  "repository": "https://github.com/andelaosp/wire-api.git",
  "author": "Wire People <@Andela>",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon ./bin/www",
    "debug": "node --inspect ./bin/www",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed --seed levels && sequelize db:seed --seed categories && sequelize db:seed --seed status && sequelize db:seed --seed locations && sequelize db:seed --seed roles && sequelize db:seed --seed incidents && sequelize db:seed --seed users && sequelize db:seed --seed reporters && sequelize db:seed --seed witnesses && sequelize db:seed --seed notificationGroups && sequelize db:seed --seed notificationTypes && sequelize db:seed --seed notifications && sequelize db:seed --seed slackUsers && sequelize db:seed --seed slackEvents",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "lint": "./node_modules/.bin/eslint --fix ./server ./test",
    "migrate-seed": "npm run migrate && npm run seed",
    "pretest": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test yarn run seed",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text jest --detectOpenHandles --runInBand",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "jest": {
    "collectCoverage": true,
    "forceExit": true
  },
  "dependencies": {
    "axios": "0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "cuid": "^2.1.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "joi": "14.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.0",
    "mysql": "2.17.1",
    "nodemailer": "6.3.0",
    "nodemailer-smtp-transport": "2.7.4",
    "nodemon": "^1.19.1",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.12.1",
    "sequelize-cli": "^5.5.0",
    "slack-error-notify": "^1.0.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.5",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-prettier": "^3.1.0",
    "istanbul": "^0.4.5",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "nodemon": "^1.14.11",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "supertest": "^4.0.2"
  }
}
